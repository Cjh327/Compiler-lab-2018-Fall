int main(){
    int count, i, valid, j, k, cont, n;
    int place[8];
    int row[8];
    int ldiag[8];
    int rdiag[8];
    count = 0;
    i = 0;
    n = read();
    while(i < n) {
        place[i] = -1;
        i = i + 1;
    }
    i = 0;
    cont = 1;
    while(cont == 1) {
        if (i == n) {
            valid = 1;
            j = 0;
            while (j < n) {
                row[j] = 1;
                ldiag[j] = 1;
                rdiag[j] = 1;
                j = j + 1;
            }
            j = 0;
            while (j < n) {
                if(row[place[j]] != 1
                || ldiag[place[j]] != 1
                || rdiag[place[j]] != 1) {
                    valid = 0;
                    j = n;
                } else {
                    row[place[j]] = 0;
                    k = 0;
                    while (k < n-1) {
                        ldiag[k]
                            = ldiag[k + 1];
                        k = k + 1;
                    }
                    ldiag[n-1] = 1;
                    if (place[j] != 0)
                        { ldiag[place[j] - 1] = 0; }
                    k = n-1;
                    while(k > 0) {
                        rdiag[k]
                            = rdiag[k - 1];
                        k = k - 1;
                    }
                    rdiag[0] = 1;
                    if (place[j] != n-1)
                        {rdiag[place[j] + 1] = 0;}
                    j = j + 1;
                }
            }
            if (valid == 1) {
                count = count + 1;
            }
            i = i - 1;
        } else {
            while (i >= 0 && place[i] >= n-1) {
                place[i] = -1;
                i = i - 1;
            }
            if (i == -1){
                cont = 0;
            } else {
                place[i] = place[i] + 1;
                i = i + 1;
            }
        }
    }
    write(count);
    return 0;
}
